<div class="user-overview user-overview-monitor psrl" ng-scroll="false, true">

	<div class="user-box">

		<div class="user-box-head clearfix">
			<span class="user-box-head-tool">
			</span>
			<h4  class="user-box-title"><i class="fa fa-info-circle"></i> 性能
				<button ng-click="showMyPerf = !showMyPerf" class="btn btn-link btn-xs psal r10" ng-init="showMyPerf = false">
					<i class="fa fa-plus" ng-show="!showMyPerf" style="font-size: 15px;"></i>
					<i class="fa fa-minus" ng-show="showMyPerf" style="font-size: 15px;"></i>
				</button>
			</h4>
		</div>

		<div class="user-box-body" ng-show="showMyPerf">
			<div class="user-monitor-filter user-monitor-clearinput clear-fix mb20 mt20 row">
				<div style="width: 15%;display: inline-block;" id="user-monitor-cicate" class="psrl">
					<label style="width: 40%;	padding: 10px;text-align: center;">CI分类</label><div class="user-monitor-filter-cicate psal" style="width: 60%;display: inline-block;"><input type="text" class="form-control typeahead" ng-model="inputCiCate" style="display: inline-block;"/></div><!-- typeahead="item as item.name for item in ciCates | filter:{name:$viewValue}"-->
				</div>
				<div style="width: 15%;display: inline-block;" id="user-monitor-ci" class="psrl">
					<label style="width: 40%;	padding: 10px;text-align: center;">CI名称</label><div class="user-monitor-filter-cicate psal" style="width: 60%;display: inline-block;"><input type="text" readonly class="form-control typeahead" ng-model="inputCi" style="display: inline-block;"/></div>
				</div>
				<div style="width: 15%;display: inline-block;" id="user-monitor-kpicate" class="psrl">
					<label style="width: 40%;	padding: 10px;text-align: center;">KPI分类</label><div class="user-monitor-filter-cicate psal" style="width: 60%;display: inline-block;"><input type="text" class="form-control typeahead" ng-model="inputKpiCate" style="display: inline-block;"/></div>
				</div>
				<div style="width: 15%;display: inline-block;" id="user-monitor-kpi" class="psrl">
					<label style="width: 40%;	padding: 10px;text-align: center;">KPI名称</label><div class="user-monitor-filter-cicate psal" style="width: 60%;display: inline-block;"><input type="text" readonly class="form-control typeahead" ng-model="inputKpi" style="display: inline-block;"/></div>
				</div>
				<div style="display: inline-block;" class="psrl">
					<div style="display: inline-block;padding-left: 30px;; width: 100%;">
						<input datetime="yyyy-MM-dd HH:mm:ss" type="text" class="form-control" placeholder="输入开始时间" ng-model="monitorSt" ng-init="monitorSt = ''" id="user-monitor-st" style="display: inline-block;width: 160px;"/>
						<span class="add-on">--</span>
						<input datetime="yyyy-MM-dd HH:mm:ss" type="text" class="form-control" placeholder="输入结束时间" ng-model="monitorEt" ng-init="monitorEt = ''" id="user-monitor-et" style="display: inline-block;width: 160px;"/>
						<span ng-click="clearInput($event);" class="ml10"><i class="fa fa-times"></i></span>
					</div>
				</div>
				<div class="clearfix psrl">
					<button class="btn btn-primary pull-right mr40" ng-click="showMonitor();">查询</button>
					<button class="btn btn-warning pull-right mr20" ng-click="clearMonitorSearch();">清空查询条件</button>
				</div>
			</div>
			<div class="user-monitor-table psrl w100"  ng-scroll="true,true">
				<table class="table w100 table-bordered text-center" ng-table="tableParams1">
					<tr>
						<th ng-repeat="title in titles">{{title}}</th>
					</tr>
					<tr ng-repeat="item in $data">
						<td ng-repeat="value in cutMonitorItem(item) track by $index" >{{value}}</td>
						<td>
							<button ng-click="showHistoryPerformance(item)" class="btn btn-link btn-xs noselect">
								<i class="fa fa-area-chart"></i>
							</button>
						</td>
					</tr>
				</table>
			</div>
		</div>
	</div>

	<div class="user-box">

		<div class="user-box-head clearfix">
			<span class="user-box-head-tool">
			</span>
			<h4  class="user-box-title"><i class="fa fa-info-circle"></i> 告警
				<button ng-click="showMyEvent = !showMyEvent" class="btn btn-link btn-xs psal r10" ng-init="showMyEvent = false">
					<i class="fa fa-plus" ng-show="!showMyEvent" style="font-size: 15px;"></i>
					<i class="fa fa-minus" ng-show="showMyEvent" style="font-size: 15px;"></i>
				</button>
			</h4>
		</div>

		<div class="user-box-body" ng-show="showMyEvent">
			<div class="user-monitor-filter clearfix mb20 mt20 mr20 ml20 row">
				<div class="mr20" style="display: inline-block;width: 200px;"><input datetime="yyyy-MM-dd HH:mm:ss" type="text" class="form-control mr10" placeholder="输入开始时间" id="user-monitor-eventst" ng-model="eventSt" ng-init="eventSt = ''" style="display: inline-block;width: 175px;" /><span ng-click="clearInput($event);"><i class="fa fa-times"></i></span></div>
				<div class="mr40" style="display: inline-block;width: 200px;"><input datetime="yyyy-MM-dd HH:mm:ss" type="text" class="form-control mr10" placeholder="输入结束时间" id="user-monitor-eventet" ng-model="eventEt" ng-init="eventEt = ''" style="display: inline-block;width: 175px;"/><span ng-click="clearInput($event);"><i class="fa fa-times"></i></span></div>
				<div class="ml40" style="display: inline-block;width: 200px;"><input type="text" class="form-control mr10" style="display: inline-block;width: 175px;" placeholder="输入查询信息" id="user-monitor-inputInfo" ng-model="eventInput" ng-init="eventSt = ''"/><span ng-click="clearInput($event);"><i class="fa fa-times"></i></span></div>
				<button class="btn btn-primary pull-right" ng-click="showEvent()">查询</button>
				<button class="btn btn-warning pull-right mr20" ng-click="clearEventSearch()">清空查询条件</button>
			</div>
			<div class="user-monitor-eventtable psrl w100" ng-scroll="true,true">
				<div class="h100 ng-isolate-scope">
					<ul class="nav nav-tabs">
						<li class="ng-isolate-scope" ng-class="{'active':isAll}">
							<a href="" ng-click="showEvent()" ><tab-heading>
								<span><i class="fa fa-circle"></i> 全部 <span class="badge tab-badge" ng-class="{'highlight': isAll}">{{allCountEvent}}</span></span>
							</tab-heading></a>
						</li>
						<li class="ng-isolate-scope" ng-class="{'active':whichSeverity[severity.id]}" ng-repeat="severity in severityCount">
							<a href="" ng-click="showEvent(severity.id)"><tab-heading>
								<span style="color: {{severity.color}}"><i class="fa fa-circle"></i> {{severity.name}} <span class="badge tab-badge" ng-class="{'highlight': whichSeverity[severity.id]}">{{severity.count}}</span></span>
							</tab-heading></a>
						</li>
					</ul>
				</div>
				<table class="table w100 table-bordered text-center mt20" ng-table="tableParams2">
					<tr>
						<th ng-repeat="eventTitle in eventTitles">{{eventTitle.name}}</th>
						<th>操作</th>
					</tr>
					<tr ng-repeat="item in $data track by $index" style="background-color: {{item.color}}">
						<td ng-repeat="item1 in cutEventItem(item) track by $index" >{{item1}}</td>
						<td>
							<span class="badge badge-success" style="cursor: pointer;" ng-click="getDetailInfo(item[0]);">详细信息</span>
							<span class="badge badge-success" data-toggle="dropdown" style="cursor: pointer;" ng-click="getViewsByEvent(item[0], $event);">事件定位</span>
						</td>
					</tr>
					<tr ng-show="!$data.length">
						<td colspan="12" class="result-ci-alert-td">
							<div class="alert alert-warning mt20 mr20 ml20 mb20"><i class="fa fa-exclamation-circle"></i> 没有数据</div>
						</td>
					</tr>
				</table>
			</div>
		</div>
	</div>

</div>
<div style="position: fixed; background-color: white; display: none" id="user-monitor-views-op" onmouseout="this.style.display = 'none'">
	<ul>
		<li ng-repeat="view in viewsByEvent"><button ng-click="toViewByEvent(view.url);">{{view.name}}</button></li>
	</ul>
</div>
