<script type="text/ng-template" id="graphinfo/ci">

	<div class="pd10 box-border">
		<div class="psrl">
			<h5 class="noselect text-primary text-capitalize">基本配置</h5>
			<span class="psal noselect t-5 r10">
				<button class="btn btn-default btn-xs"  ng-click="callapse.config = !callapse.config;">
					<i class="fa fa-angle-down" ng-if="!callapse.config"></i>
					<i class="fa fa-angle-left" ng-if="callapse.config"></i>
				</button>
			</span>
		</div>
		<ul class="info-row-list" ng-show="!callapse.config">
			<li class="info-row">
				<span class="info-row-label">视图名称</span>
				<span class="info-row-content">{{ params.data.config.name }}</span>
			</li>
			<li class="info-row">
				<span class="info-row-label">所属分类</span>
				<span class="info-row-content">{{ params.data.config.categoryName }}</span>
			</li>
			<li class="info-row" ng-repeat="(key, value) in params.data.config.data">
				<span class="info-row-label">{{ key }}</span>
				<span class="info-row-content">{{ value }}</span>
			</li>
		</ul>
	</div>

	<div class="pd10 box-border" ng-show="params.data.event">
		<div class="psrl">
			<h5 class="noselect text-primary text-capitalize">事件</h5>
			<span class="psal noselect t-5 r10">
				<button class="btn btn-default btn-xs"  ng-click="callapse.event = !callapse.event;">
					<i class="fa fa-angle-down" ng-if="!callapse.event"></i>
					<i class="fa fa-angle-left" ng-if="callapse.event"></i>
				</button>
			</span>
		</div>

		<div ng-show="!callapse.event">
			<table class="table">
				<tr>
					<td ng-repeat="item in params.data.event.head">{{item}}</td>
				</tr>
				<tr ng-repeat="row in params.data.event.row">
					<td ng-repeat="item in params.data.event.head">{{ row[item] }}</td>
				</tr>
			</table>
		</div>
	</div>

	<div class="pd10 box-border" ng-show="params.data.perf">
		<div class="psrl">
			<h5 class="noselect text-primary text-capitalize">监控</h5>
			<span class="psal noselect t-5 r10">
				<button class="btn btn-default btn-xs"  ng-click="callapse.perf = !callapse.perf;">
					<i class="fa fa-angle-down" ng-if="!callapse.perf"></i>
					<i class="fa fa-angle-left" ng-if="callapse.perf"></i>
				</button>
			</span>
		</div>

		<div ng-show="!callapse.perf">
			<table class="table">
				<tr>
					<td ng-repeat="item in params.data.perf.head">{{item}}</td>
				</tr>
				<tr ng-repeat="row in params.data.perf.row">
					<td ng-repeat="item in params.data.perf.head">{{ row[item] }}</td>
				</tr>
			</table>
		</div>
	</div>
</script>
