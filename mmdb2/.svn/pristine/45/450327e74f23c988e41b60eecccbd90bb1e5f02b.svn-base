<div class="user-head-image">
	<h1>this is user head image</h1>

	<div class="w100 clearfix"
		 ngf-select="true"
		 ngf-drop
		 ngf-accept="'image/*,.svg'">

		<div ng-show="!icons.length" class="alert alert-warning user-icon-drop-area">

			<div class="user-upload-word-div pull-left">
				文字描述区
			</div>
			<div class="user-upload-icon-div pull-right">
				<img src="{{static(userInfo.icon)}}" />
			</div>
		</div>
	</div>
	<button class="btn btn-success">上传</button>
</div>
