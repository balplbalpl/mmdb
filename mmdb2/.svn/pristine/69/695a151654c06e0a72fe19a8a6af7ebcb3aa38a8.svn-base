<script type="text/ng-template" id="graphinfo/view">
	<ul class="info-row-list">
		<li class="info-row">
			<span class="info-row-label">视图名称</span>
			<span class="info-row-content">{{ params.data.name }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">所属分类</span>
			<span class="info-row-content">{{ params.data.categoryName }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">是否公开</span>
			<span class="info-row-content">{{ formatBool(params.data.open) }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">创建日期</span>
			<span class="info-row-content">{{ formatDate(params.data.createTime) }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">更新日期</span>
			<span class="info-row-content">{{ formatDate(params.data.updateTime) }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">视图描述</span>
			<span class="info-row-content">{{ params.data.description }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">视图版本</span>
			<span class="info-row-content">{{ params.data.version }}</span>
		</li>
		<li class="info-row">
			<span class="info-row-label">CI数量</span>
			<span class="info-row-content">{{ params.data.ciSize }}</span>
		</li>

		<li class="info-row">
			<span class="info-row-label">作者</span>
			<span class="info-row-content">{{ params.data.userName }}</span>
		</li>

	</ul>
</script>
