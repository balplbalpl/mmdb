<tr class="after">
	<td class="details" colspan="20">
		<div>
			<div class="col-md-12">
				{{#each list}}
				<div class="col-md-3 mt10">{{key}}:{{val}}</div>
				{{/each}}
			</div>

            <div class="row text-right">
                <button class="btn btn-default icCopyData">
                    复制<i class="fa fa-plus"></i>
                </button>

                <button class="btn btn-success icEditorData">
                    编辑<i class="fa fa-plus"></i>
                </button>

                <button class="btn btn-danger icDelData">
                    删除<i class="fa fa-plus"></i>
                </button>
            </div>
		</div>
	</td>
</tr>
